{
  "scopeName": "source.lay",
  "name": "FileIO File Layouts",
  "fileTypes":
  [
    "lay",
    "fio"
  ],
  "patterns":
  [
    {
      "match": "(!.*)$",
      "name": "comment.line.br"
    },
    {
      "match": "\\b(?i:#EOF#)\\b",
      "name": "keyword.control.br"
    },
    {
      "match": "\\b(?i:date\\((julian|serial|(mm|dd|cc|yy|m|d|c|y)(-|\\/|)(mm|dd|cc|yy|m|d|c|y)(-|\\/|)(mm|dd|cc|yy|m|d|c|y)(-|\\/|)(mm|dd|cc|yy|m|d|c|y|))\\))\\b",
      "name": "keyword.other.br"
    }
  ]
}
